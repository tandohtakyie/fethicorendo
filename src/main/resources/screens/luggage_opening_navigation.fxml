<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8.0.152" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.corendon.luggage_finder.controllers.LuggageOpeningNavigationController">
    <top>
        <ToolBar BorderPane.alignment="CENTER">
            <items>
                <Button mnemonicParsing="false" onAction="#onBackButtonAction" style="-fx-background-color: #26a514;" text="%general_button_back" textFill="WHITE" />
                <Label text="%luggage_opening_navigation_toolbar_title" textFill="#d81e05" />
            </items>
        </ToolBar>
    </top>
    <center>
        <TabPane tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
            <tabs>
                <Tab text="%luggage_opening_navigation_tab_info">
                    <content>
                        <AnchorPane fx:id="infoAnchorPane" minHeight="0.0" minWidth="0.0" />
                    </content>
                </Tab>
                <Tab text="%luggage_opening_navigation_tab_status">
                    <content>
                        <AnchorPane fx:id="statusAnchorPane" minHeight="0.0" minWidth="0.0" />
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </center>
</BorderPane>
